    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-chrome-extension": "^0.0.7",
    "vitest": "^0.23.4",
    "web-ext": "^7.4.0"
