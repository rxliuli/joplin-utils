{
  "name": "i18next-typescript-generator",
  "version": "0.1.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "setup": "yarn pub",
    "build": "liuli-cli clean && tsc -p tsconfig.build.json && liuli-cli addCliBanner && liuli-cli build",
