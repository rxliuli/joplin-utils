  "bin": {
    "i18next-dts-gen": "dist/bin.js"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "@liuli-util/array": "^3.2.2",
    "@liuli-util/async": "^3.2.2",
    "commander": "^7.2.0",
    "fs-extra": "^9.1.0"
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.2.2",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^12.20.7",
    "typescript": "^4.2.4"
  }
}
