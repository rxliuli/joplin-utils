{
  "name": "@liuli-util/i18next-util",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "setup": "yarn gen-i18n",
    "build": "rm dist && liuli-cli build pkg",
    "dev": "liuli-cli build pkg -w",
    "docs:server": "live-server docs",
    "docs:dev": "typedoc --watch",
    "docs:build": "rm docs && typedoc",
    "docs:deploy": "yarn build && gh-pages -d docs/ -e / -a",
    "gen-i18n": "i18next-dts-gen --input src/__tests__/i18n"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "peerDependencies": {
    "i18next": "^20.3.3"
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.4.0",
    "@liuli-util/shell": "^0.2.0",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "type-fest": "^1.2.1",
    "typedoc": "^0.21.0",
    "typescript": "^4.3.4"
  }
}
